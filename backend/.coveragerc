[run]
source = app
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    */.venv/*
    */site-packages/*
    */conftest.py
    */mocks/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Reactrine Backend Coverage Report

[xml]
output = coverage.xml
